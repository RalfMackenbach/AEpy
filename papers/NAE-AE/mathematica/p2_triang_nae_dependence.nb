Cell[TextData[{
 "From [Landreman, \[OpenCurlyDoubleQuote]Magnetic well and Mercier stability \
of stellarators near the magnetic axis\[CloseCurlyDoubleQuote] (2020)],\n",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         SubscriptBox[
          StyleBox["D", "TI"], 
          StyleBox["Merc", FontSlant -> "Plain"]], "\[LongEqual]", 
         FractionBox[
          RowBox[{"|", 
            SubscriptBox[
             StyleBox["G", "TI"], "0"], "|", 
            SubscriptBox["\[Mu]", "0"], 
            SubscriptBox[
             StyleBox["p", "TI"], "2"]}], 
          RowBox[{"8", 
            SuperscriptBox["\[Pi]", "4"], 
            SuperscriptBox["\[Epsilon]", "2"], 
            SubsuperscriptBox[
             StyleBox["B", "TI"], "0", "3"]}]], 
         RowBox[{"[", 
           RowBox[{
             SuperscriptBox[
              StyleBox["V", "TI"], "\[Prime]\[Prime]"], "-", 
             FractionBox[
              RowBox[{"8", 
                SuperscriptBox["\[Pi]", "2"], "|", 
                SubscriptBox[
                 StyleBox["G", "TI"], "0"], "|", 
                SubscriptBox["\[Mu]", "0"], 
                SubscriptBox[
                 StyleBox["p", "TI"], "2"]}], 
              SubsuperscriptBox[
               StyleBox["B", "TI"], "0", "5"]], "-", 
             FractionBox[
              RowBox[{"32", 
                SuperscriptBox["\[Pi]", "2"], "|", 
                SubscriptBox[
                 StyleBox["G", "TI"], "0"], 
                SuperscriptBox["|", "3"], 
                SubscriptBox["\[Mu]", "0"], 
                SubscriptBox[
                 StyleBox["p", "TI"], "2"], 
                SuperscriptBox["\[Eta]", "2"]}], 
              RowBox[{
                SubsuperscriptBox[
                 StyleBox["B", "TI"], "0", "7"], 
                SubsuperscriptBox[
                 OverscriptBox["\[Iota]", "_"], "0", "2"]}]], 
             StyleBox["I", "TI"]}], "]"}]}], TraditionalForm], "errors" -> {},
      "input" -> 
     "D_\\mathrm{Merc}=\\frac{|G_0|\\mu_0 p_2}{8\\pi^4\\epsilon^2 B_0^3}[V''-\
\\frac{8\\pi^2|G_0|\\mu_0p_2}{B_0^5}-\\frac{32\\pi^2|G_0|^3\\mu_0p_2\\eta^2}{\
B_0^7\\bar{\\iota}_0^2}I]", "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "cfb05f9d-be15-4138-9dbe-d8bc21497746"],
