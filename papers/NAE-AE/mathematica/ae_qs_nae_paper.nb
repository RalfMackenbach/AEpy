 RowBox[{
  RowBox[{"deltaB", "=", 
   RowBox[{
    SuperscriptBox["r", "2"], 
    RowBox[{"(", 
     RowBox[{"B20", "+", 
      RowBox[{"B2c", " ", 
       RowBox[{"Cos", "[", 
        RowBox[{"2", "\[Chi]"}], "]"}]}]}], ")"}]}]}], ";"}]], "Input",
