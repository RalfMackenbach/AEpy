 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{"\[Lambda]", "\[LongEqual]", 
         FractionBox[
          RowBox[{"\[Mu]", 
            SubscriptBox[
             StyleBox["B", "TI"], "0"]}], 
          StyleBox["H", "TI"]]}], TraditionalForm], "errors" -> {}, "input" -> 
     "\\lambda=\\frac{\\mu B_0}{H}", "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "6dd7d88d-e879-430b-9e52-b000b28b35b1"],
 ","
